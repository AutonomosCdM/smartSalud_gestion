<script>jQuery(function($){
  var tabMap = {
    home: 90, menu4:93, menu3:97, menu2:92,
    menu1:91, menu5:95, menu6:96
  };
  var tables = {};
  $.each(tabMap, function(key,id){
    tables[key] = $('#tablepress-'+id).DataTable({
      responsive:true,
      language:{ search:"Buscar:", lengthMenu:"Mostrar _MENU_", info:" _START_–_END_ de _TOTAL_" }
    });
  });
  function updateSummary(key){
    var title = $('button[data-bs-target="#'+key+'"]').text();
    $('#guides-redesign .card-summary h5').text(title);
    $('#total-count').text(tables[key].data().count());
    tables[key].columns.adjust().responsive.recalc();
  }
  updateSummary('home');
  $('button[data-bs-toggle="pill"]').on('shown.bs.tab',function(e){
    var k = $(e.target).data('bs-target').substring(1);
    updateSummary(k);
    $('#cardsContainer').empty().addClass('d-none');
    $('#tableContainer').removeClass('d-none');
    $('#toggleView').text('Ver como tarjetas');
  });
  $('#applyFilters').click(function(){
    var year = $('#filter-year').val();
    var active = $('.nav-pills .active').data('bs-target').substring(1);
    tables[active].column(2).search(year).draw();
    bootstrap.Offcanvas.getInstance($('#filterPanel')[0]).hide();
    updateSummary(active);
  });
  $('#toggleView').click(function(){
    var active = $('.nav-pills .active').data('bs-target').substring(1),
        dt = tables[active];
    if ($('#tableContainer').is(':visible')) {
      $('#tableContainer').addClass('d-none');
      $('#cardsContainer').empty().removeClass('d-none');
      dt.rows({search:'applied'}).data().each(r=>{
        $('#cardsContainer').append(`
          <div class="col-md-4"><div class="card h-100 mb-3">
          <div class="card-body">${r[1]}<p><strong>Año:</strong> ${r[2]}</p>
          </div></div></div>`);
      });
      $(this).text('Ver como tabla');
    } else {
      $('#cardsContainer').addClass('d-none').empty();
      $('#tableContainer').removeClass('d-none');
      $(this).text('Ver como tarjetas');
    }
  });
});
</script><script>document.addEventListener('DOMContentLoaded', function(){
  const carousel = document.querySelector('.doc-carousel');
  if (!carousel) return;

  const prevBtn = document.querySelector('.carousel-prev');
  const nextBtn = document.querySelector('.carousel-next');

  // calculamos gap + ancho de tarjeta
  const slide     = carousel.querySelector('.doc-slide');
  const style     = getComputedStyle(carousel);
  const gap       = parseInt(style.gap) || 16;
  const width     = slide.getBoundingClientRect().width;
  const scrollAmt = width + gap;

  // límite máximo de scroll
  const maxScroll = carousel.scrollWidth - carousel.clientWidth;

  nextBtn.addEventListener('click', () => {
    if (carousel.scrollLeft >= maxScroll - 1) {
      // si ya estás al final, vuelve al principio
      carousel.scrollTo({ left: 0, behavior: 'smooth' });
    } else {
      carousel.scrollBy({ left: scrollAmt, behavior: 'smooth' });
    }
  });

  prevBtn.addEventListener('click', () => {
    if (carousel.scrollLeft <= 0) {
      // si ya estás al principio, ve al final
      carousel.scrollTo({ left: maxScroll, behavior: 'smooth' });
    } else {
      carousel.scrollBy({ left: -scrollAmt, behavior: 'smooth' });
    }
  });
});
</script><style class="wpcode-css-snippet">/* 1) Contenedor horizontal con scroll-snap */
.doc-carousel {
  display: flex;
  gap: 1rem;
  overflow-x: auto;
  scroll-snap-type: x mandatory;
  /* Oculta la barra sin desactivar el scroll */
  scrollbar-width: none;            /* Firefox */
  -ms-overflow-style: none;         /* IE 10+ */
}
.doc-carousel::-webkit-scrollbar {  /* Chrome, Safari y Edge */
  display: none;
}

/* Título del carrusel: tamaño más compacto */
.doc-carousel-title {
  display: inline-block;        /* Para que el fondo envuelva solo el texto */
  --background-color: #0f69b4;    /* Mismo azul de tu botón */
  background-color: #fff;
  color: #337ab7!important;
  padding: 15px;
  --color: #ffffff;               /* Texto blanco */
  padding: 0.5em 1em;           /* Espaciado arriba/abajo e izquierda/derecha */
  border-radius: 4px;           /* Esquinas redondeadas */
  font-size: 1.25rem;           /* Ajusta tamaño si lo ves muy grande/pequeño */
  font-weight: 500;             /* Un poco más grueso */
  margin-bottom: 1rem;          /* Separación con el carrusel */
  display: block;
  width: 100%;
  --text-align: center;           /* Centrar el texto */
}


/* Scrollbar ligero (Webkit) */
.doc-carousel::-webkit-scrollbar {
  height: 6px;
}
.doc-carousel::-webkit-scrollbar-thumb {
  background: rgba(0,0,0,.2);
  border-radius: 3px;
}

/* 2) Cada “slide” encaja */
.doc-slide {
  scroll-snap-align: start;
  flex: 0 0 calc(25% - 1rem); /* 4 por fila en desktop */
  box-sizing: border-box;
  background: #fff;
  border: 1px solid #e0e0e0;
  border-radius: 4px;
  overflow: hidden;
  display: flex;
  box-shadow: 0 2px 8px rgba(0,0,0,0.1);
  flex-direction: column;
}

/* Imágenes a full ancho */
.doc-slide img {
  width: 100%;
  height: auto;
  display: block;
}

/* Cuerpo de la “card” */
.doc-slide .card-body {
  padding: .5rem;
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}
.doc-slide h5 {
  font-family: system-ui;
  margin: 0;
  font-size: .9rem;
  text-align: center;
}
.doc-slide h5 a {
  color: #0f69b4;
  text-decoration: none;
}
.doc-slide h5 a:hover {
  text-decoration: underline;
}
/* Contenedor con posición relativa */
.doc-carousel-wrapper {
  position: relative;
  margin: 1rem 0;
}

/* Flechas */
.carousel-arrow {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  z-index: 10;
  width: 2rem;
  height: 2rem;
  border: none;
  background: rgba(0,0,0,0.3);
  color: #fff;
  font-size: 1.5rem;
  line-height: 2rem;
  border-radius: 50%;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
}
.carousel-prev { left: 0.25rem; }
.carousel-next { right: 0.25rem; }
.carousel-arrow:hover {
  background: rgba(0,0,0,0.5);
}

/* Asegura que el scrollbar quede oculto tras las flechas */
.doc-carousel {
  padding: 0 2.5rem;
}

/* 3) Responsive */
@media (max-width: 992px) {
  .doc-slide {
    flex: 0 0 calc(33.333% - 1rem); /* 3 por fila en tablet */
