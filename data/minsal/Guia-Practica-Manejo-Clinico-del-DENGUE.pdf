<script>
document.addEventListener('DOMContentLoaded', function() {
    document.querySelectorAll('a[href$=".pdf"], a[href$=".docx"], a[href$=".xlsx"]').forEach(link => {
        link.addEventListener('click', function(event) {
            var fileUrl = this.href;
            // Enviar evento a Google Analytics
            gtag('event', 'download', {
                'event_category': 'Documentos',
                'event_label': fileUrl,
                'transport_type': 'beacon'
            });
            // Opcional: Retraso para asegurar que el evento se envíe antes de la navegación
            if (!link.target || link.target.toLowerCase() !== "_blank") {
                event.preventDefault();
                setTimeout(function() {
                    window.location.href = fileUrl;
                }, 800); // Retraso de 800 ms
            }
        });
    });
});
</script>

